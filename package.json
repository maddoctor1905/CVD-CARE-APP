{
  "name": "@enoviah/mhealth",
  "version": "0.2.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build:prod": "ng build --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "setup": "npm i"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.13",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~8.2.13",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/pwa": "^0.803.22",
    "@angular/router": "~8.2.13",
    "@angular/service-worker": "~8.2.13",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bulma": "^0.8.0",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular/cli": "~8.3.18",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^4.0.6",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "description": "Lorem ipsum dolor sit amet, consectetur adipiscing elit.",
  "author": "Username aka JDoe",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "enoviah": {
    "configuration": {
      "lifecycle": {
        "macos": {
          "onInit": {
            "commands": []
          },
          "onArtifact": {
            "commands": []
          },
          "onCodeQuality": {
            "commands": []
          }
        },
        "linux": {
          "onInit": {
            "commands": []
          },
          "onArtifact": {
            "commands": []
          },
          "onCodeQuality": {
            "commands": []
          }
        }
      },
      "rootPath": ".",
      "type": "service",
      "name": "mhealth",
      "group": "enoviah"
    },
    "service": {
      "artifact": {
        "linux": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [
            {
              "path": "Dockerfile",
              "name": "Dockerfile"
            },
            {
              "path": "package.json",
              "name": "package.json"
            },
            {
              "path": "docker",
              "name": "docker"
            }
          ],
          "test": false
        },
        "macos": {
          "coverage": false,
          "e2e": false,
          "lint": false,
          "mandatory": [],
          "test": false
        }
      },
      "deployment": {
        "production": {
          "network": "traefik",
          "labels": {
            "traefik.port": "80",
            "traefik.frontend.rule": "Host:mit.enoviah.fr",
            "traefik.frontend.entryPoints": "http,https",
            "traefik.backend": "mhealth"
          },
          "env": []
        },
        "staging": {
          "network": "traefik",
          "labels": {
            "traefik.port": "80",
            "traefik.frontend.rule": "Host:mit.enoviah.staging",
            "traefik.frontend.entryPoints": "http,https",
            "traefik.backend": "mhealth"
          },
          "env": []
        }
      }
    }
  }
}
